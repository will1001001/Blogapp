{{#each Postagens}}
    <div class="card mt-4">
        <div class="card-body">
            <hr>
            <h3>{{titulo}}</h3>
            <p>{{descricao}}</p>
            <a href="/postagem/{{slug}}"><button class="btn btn-success">Leia mais</button></a>
            <hr>
            <small>Data publicação{{data}}</small>
        </div>
    </div>
    {{else}}
    <p>Nenhuma postagem</p>
{{/each}}